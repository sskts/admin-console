<!-- Content Header (Page header) -->
<div class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-sm-6">
                <h1 class="m-0 text-dark">出金取引</h1>
            </div>
            <!-- /.col -->
            <div class="col-sm-6">
                <ol class="breadcrumb float-sm-right">
                    <li class="breadcrumb-item">
                        <a href="/">Home</a>
                    </li>
                    <li class="breadcrumb-item active">出金取引</li>
                </ol>
            </div>
            <!-- /.col -->
        </div>
        <!-- /.row -->
    </div>
    <!-- /.container-fluid -->
</div>
<!-- /.content-header -->

<!-- Main content -->
<section class="content">
    <div class="container-fluid">
        <div class="row">
            <!-- left column -->
            <div class="col-md-12">
                <!-- general form elements -->
                <div class="card card-primary">
                    <!-- <div class="card-header">
                        <h3 class="card-title">Quick Example</h3>
                    </div> -->
                    <!-- /.card-header -->

                    <!-- form start -->
                    <form role="form" method="post">
                        <div class="card-body">
                            <% if (messages.message) { %>
                                <div class="alert alert-success alert-dismissible">
                                    <button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>
                                    <h5>
                                        <i class="icon fa fa-check"></i> Congraturations!</h5>
                                    <%= messages.message %>
                                </div>
                                <% } %>

                                    <div class="form-group">
                                        <label for="fromName">出金者名</label>
                                        <input type="text" class="form-control" id="fromName" name="fromName" placeholder="出金者名" value="<%= values.fromName %>">
                                    </div>
                                    <div class="form-group">
                                        <label for="fromAccountNumber">出金元口座番号</label>
                                        <input type="text" class="form-control" id="fromAccountNumber" name="fromAccountNumber" placeholder="出金元口座番号" value="<%= values.fromAccountNumber %>">
                                    </div>
                                    <div class="form-group">
                                        <label for="amount">出金金額</label>
                                        <input type="text" class="form-control" id="amount" name="amount" placeholder="出金金額" value="<%= values.amount %>">
                                    </div>
                                    <div class="form-group">
                                        <label for="notes">説明</label>
                                        <input type="text" class="form-control" id="notes" name="notes" placeholder="説明" value="<%= values.notes %>">
                                    </div>
                        </div>
                        <!-- /.card-body -->

                        <div class="card-footer">
                            <button type="submit" class="btn btn-primary">確認</button>
                        </div>
                    </form>
                </div>
                <!-- /.card -->

            </div>
            <!--/.col (left) -->
        </div>
        <!-- /.row -->
    </div>
    <!-- /.container-fluid -->
</section>
<!-- /.content -->

<!-- page script -->
<script>
</script>