        "build": "tsc",
        "check": "npm run tslint",
        "clean": "rimraf ./dist ./docs ./coverage \"npm-debug.log*\" ./.nyc_output",
        "doc": "echo \"implementing...\" && exit 0",
        "mocha": "mocha",
        "nyc": "nyc mocha",
        "coverage": "npm run nyc -- \"src/**/*.spec.ts\"",
        "start": "node ./dist/bin/www",
        "start:development": "node ./dist/bin/ssl",
        "test": "npm run check",
        "tslint": "tslint --project tsconfig.json -c tslint.json src/**/*.ts --exclude **/*.spec.ts",
        "preversion": "npm run clean && npm run build && npm test && npm run doc",
        "version": "git add -A",
        "postversion": "git push origin --tags",
        "deploy:development": "gcloud app deploy ./app.development.gcp.yaml --project sskts-dev",
        "deploy:test": "gcloud app deploy ./app.test.gcp.yaml --project sskts-test",
        "deploy:production": "gcloud app deploy ./app.production.gcp.yaml --project sskts-prod-admin --no-promote --version staging"
